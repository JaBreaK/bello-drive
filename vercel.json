{
  "version": 2,
  "framework": null,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install && cd client && npm install",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/upload/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/files/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/files",
      "destination": "/api/index.js"
    },
    {
      "source": "/download/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/auth",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/status",
      "destination": "/api/index.js"
    }
  ]
}